{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --tunnel --go",
    "start:go": "expo start --tunnel --go",
    "start:local": "expo start --go",
    "start-web": "expo start --web --tunnel",
    "start-web-dev": "DEBUG=expo* expo start --web --tunnel",
    "clean": "rm -rf node_modules .expo .cache && npm install",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "security:audit": "npm audit --production",
    "security:audit:fix": "npm audit fix --production",
    "security:audit:backend": "cd backend && npm audit --production",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:ios": "eas build --platform ios --profile production",
    "build:ios:clean": "eas build --platform ios --profile production --clear-cache",
    "build:ios:submit": "eas build --platform ios --profile production --clear-cache --auto-submit",
    "submit:ios": "eas submit --platform ios"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@nkzw/create-context-hook": "1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.76.1",
    "@types/react": "~19.1.10",
    "expo": "~54.0.20",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "^6.0.16",
    "expo-document-picker": "~14.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.8",
    "lottie-react-native": "^7.3.4",
    "lucide-react-native": "^0.562.0",
    "nativewind": "^4.2.1",
    "pretty-format": "^29.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-plaid-link-sdk": "^12.5.1",
    "react-native-purchases": "^8.1.6",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "^0.21.0",
    "semver": "^7.7.2",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@expo/ngrok": "^4.1.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "~29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "private": true
}

{
  "expo": {
    "name": "Trilo",
    "slug": "trilo-personal-finance-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Splashscreen-Trilo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#4E91F9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.ios-personal-finance-tracker",
      "splash": {
        "image": "./assets/images/Splashscreen-Trilo.jpg",
        "resizeMode": "contain",
        "backgroundColor": "#4E91F9"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.ios-personal-finance-tracker",
      "splash": {
        "image": "./assets/images/Splashscreen-Trilo.jpg",
        "resizeMode": "contain",
        "backgroundColor": "#4E91F9"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#4E91F9",
          "image": "./assets/images/Splashscreen-Trilo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}

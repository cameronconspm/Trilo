# Trilo Frontend Environment Variables
# Copy this file to .env.local and fill in your values
# Note: .env.local is git-ignored and should never be committed

# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/_/settings/api
# The anon key is safe to expose (designed for client-side use)
EXPO_PUBLIC_SUPABASE_URL=https://raictkrsnejvfvpgqzcq.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Plaid API URL
# Your backend Plaid API endpoint
EXPO_PUBLIC_PLAID_API_URL=https://trilo-production.up.railway.app/api/plaid

# RevenueCat API Keys
# Get these from: https://app.revenuecat.com > Settings > API Keys
# IMPORTANT: Never commit these keys to version control
# Use EAS Secrets for production builds:
#   eas secret:create --scope project --name EXPO_PUBLIC_REVENUECAT_API_KEY_IOS --value your_key
#   eas secret:create --scope project --name EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID --value your_key
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your_ios_key_here
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your_android_key_here

# Note: For production builds with EAS, use:
# eas secret:create --scope project --name EXPO_PUBLIC_REVENUECAT_API_KEY_IOS --value your_key
# eas secret:create --scope project --name EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID --value your_key
